{
  "name": "arcmage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "design": "ng serve -o -c design",
    "prebuild": "del-cli --force ../Arcmage.Server.Api/wwwroot/assets && del-cli --force ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "build": "ng build",
    "postbuild": "cpx ./dist/*.* ../Arcmage.Server.Api/wwwroot/ -C && cpx ./dist/assets/**/*.* ../Arcmage.Server.Api/wwwroot/assets && cpx ./dist/arcmage/Game/**/*.* ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "prebuildlinux": "del-cli --force ../Arcmage.Server.Api/wwwroot/assets && del-cli --force ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "buildlinux": "ng build",
    "postbuildlinux": "cp -fR ./dist/*.* ../Arcmage.Server.Api/wwwroot/ && mkdir ../Arcmage.Server.Api/wwwroot/assets && cp -fR ./dist/assets ../Arcmage.Server.Api/wwwroot && mkdir ../Arcmage.Server.Api/wwwroot/arcmage/Game && cp -fR ./dist/arcmage/Game ../Arcmage.Server.Api/wwwroot/arcmage",
    "prebuildprod": "del-cli --force ../Arcmage.Server.Api/wwwroot/assets && del-cli --force ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "buildprod": "ng build --configuration=production",
    "postbuildprod": "cpx ./dist/*.* ../Arcmage.Server.Api/wwwroot/ -C && cpx ./dist/assets/**/*.* ../Arcmage.Server.Api/wwwroot/assets && cpx ./dist/arcmage/Game/**/*.* ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "prebuildgame": "del-cli --force ../Arcmage.Server.Api/wwwroot/assets && del-cli --force ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "buildgame": "cpx ./dist/*.* ../Arcmage.Server.Api/wwwroot/ -C && cpx ./dist/assets/**/*.* ../Arcmage.Server.Api/wwwroot/assets && cpx ./dist/arcmage/Game/**/*.* ../Arcmage.Server.Api/wwwroot/arcmage/Game",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.4.0",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.4.0",
    "@angular/compiler": "13.4.0",
    "@angular/core": "13.4.0",
    "@angular/forms": "13.4.0",
    "@angular/localize": "13.4.0",
    "@angular/platform-browser": "13.4.0",
    "@angular/platform-browser-dynamic": "13.4.0",
    "@angular/router": "13.4.0",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-free": "5.13.0",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@fullcalendar/core": "4.4.2",
    "@microsoft/signalr": "5.0.2",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/uuid": "7.0.2",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "core-js": "3.8.3",
    "file-saver": "1.3.3",
    "guid-typescript": "1.0.9",
    "jquery": "3.4.1",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "ngx-device-detector": "3.0.0",
    "ngx-slick-carousel": "15.0.0",
    "oidc-client": "^1.11.5",
    "popper.js": "1.15.0",
    "primeflex": "2.0.0",
    "primeicons": "^5.0.0",
    "primeng": "13.4.1",
    "quill": "^1.3.7",
    "rxjs": "6.5.5",
    "slick-carousel": "1.8.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.11",
    "@angular/cli": "13.3.11",
    "@angular/compiler-cli": "13.4.0",
    "@angular/language-service": "13.4.0",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "4.14.168",
    "@types/node": "12.19.15",
    "codelyzer": "^6.0.2",
    "cpx": "1.5.0",
    "del-cli": "3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "svg-sprite": "^1.5.3",
    "svgo": "^1.3.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.6.4"
  }
}